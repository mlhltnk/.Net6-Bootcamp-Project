{
    //JWT yapılandırması
    "TokenOptions": {
        "Audience": "melih@melih.com",
        "Issuer": "melih@melih.com",
        "AccessTokenExpiration": 10,                            //Token'ın geçerlilik süresi(10 dk)
        "SecurityKey": "mysupersecretkeymysupersecretkey"      //Tokenı oluştururken aspNet'in kullanacağı anahtarın ismi        
    },



    //loglama yapılandırması (defaultta yapmışlar)
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
